{
  "name": "kn",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "npm run watch",
    "build": "tsc -p app/tsconfig.dev.json",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "watch": "ts-node --project app/tsconfig.dev.json app/src/index.ts",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx app/src"
  },
  "author": "Stepan Savelyev",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "app/src"
    ],
    "exec": "npm start",
    "events": {
      "restart": "fuser -k 3000/tcp"
    },
    "delay": 1500,
    "ext": "ts"
  },
  "dependencies": {
    "@popperjs/core": "^2.1.1",
    "bootstrap": "^4.4.1",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "mongodb": "^3.5.5",
    "morgan": "^1.9.1",
    "nunjucks": "^3.2.1",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/mongodb": "^3.5.2",
    "@types/morgan": "^1.9.0",
    "@types/nunjucks": "^3.1.3",
    "@types/socket.io": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2"
  }
}
